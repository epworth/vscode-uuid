{
  "name": "vscode-uuid",
  "version": "1.0.1",
  "description": "A VS Code extension to generate UUIDs.",
  "main": "dist/extension.js",
  "browser": "dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ludwhe/vscode-uuid.git"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "node ./dist/test/runTest.js",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "npm run package"
  },
  "author": "ludwhe",
  "license": "ISC",
  "dependencies": {
    "uuid": "^8.3.2",
    "vscode-nls": "^5.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^16.11.6",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.61.0",
    "@vscode/test-electron": "^1.6.2",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vsce": "^2.2.0",
    "vscode-nls-dev": "^3.3.2",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "activationEvents": [
    "onCommand:vscodeUUID.generateUUID"
  ],
  "contributes": {
    "commands": [
      {
        "category": "UUID",
        "command": "vscodeUUID.generateUUID",
        "title": "Generate new UUID"
      }
    ],
    "configuration": {
      "title": "UUID Generator",
      "properties": {
        "vscodeUUID.UUIDVersion": {
          "description": "Specifies the type of UUID to generate.",
          "type": "string",
          "default": "v4",
          "enum": [
            "v1",
            "v3",
            "v4",
            "v5"
          ],
          "enumDescriptions": [
            "Generates MAC-address-and-timestamp- based (UUIDv1) UUIDs",
            "Generates namespace name-based (MD5, UUIDv3) UUIDs",
            "Generates random (UUIDv4) UUIDs",
            "Generates namespace name-based (SHA-1, UUIDv5) UUIDs"
          ]
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "vscodeUUID.generateUUID"
        }
      ]
    }
  },
  "displayName": "UUID Generator",
  "engines": {
    "vscode": "^1.61.0"
  },
  "publisher": "ludwhe"
}
